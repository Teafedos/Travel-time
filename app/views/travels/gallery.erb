<div class="gallery_page image_bg">
<p class="title_gallery">OUR TRAVELS</p>
<% @photos.each do |photo|%>
<div class="gallery_list">
    <div class="card" style="min-width: 60%; max-width: 80%;">
        
            <img src="/assets/Gallery/<%= photo.name %>.jpg" class="card-img-top" alt="..." width=30%>
            <div class="card-body body-card-gallery">
                <p class="card-text text-card-gallery"><%= photo.description %></p>
            </div>
    </div>
</div>
<br>
<% end %>
</div>